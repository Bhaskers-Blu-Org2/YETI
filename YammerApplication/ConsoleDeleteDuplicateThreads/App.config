    <add key="newbatchcount" value="5" />
    <add key="resubmitbatchcount" value="10" />
    <add key="RobocopyMoveCommandTemplate" value="&quot;{0}&quot; &quot;{1}&quot; /E /S /Z /DCOPY:T /MOVE /COPY:DAT /LOG+:&quot;{2}&quot; /R:5 /W:5 /NFL /NDL" />
