    <add key="YammerAcctNameURL" value="{YammerServiceAccountName - We need Service Account Name and Password to connect to Sharepoint.}" />
    <add key="YammerAcctPWDURL" value="{YammerServiceAccountPassword - Proivde above service account password here}" />
    <add key="SP_targetLibrary" value="DevDayYammer2018" />
